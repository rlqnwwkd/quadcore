<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
    
<mapper namespace="productSQL">
    
 	 <select id="selectProduct" parameterType="int" resultType="productDTO">
    	SELECT * FROM PRODUCT WHERE ID=#{id}
    </select>   
    
</mapper>
